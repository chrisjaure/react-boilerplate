{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React ES6 Browserify boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "browserify index.js -t 6to5ify | uglifyjs --compress > build.js",
    "watch-js": "watchify index.js -t 6to5ify --debug --verbose -o build.js",
    "dev": "npm run watch-js & browser-sync start --server --files \"build.js, index.html\"",
    "build": "npm run build-js"
  },
  "keywords": [
    "boilerplate",
    "react",
    "es6"
  ],
  "author": "Chris Jaure",
  "license": "ISC",
  "dependencies": {
    "immutable": "^3.6.2",
    "react": "^0.13.0-beta.1"
  },
  "devDependencies": {
    "6to5ify": "^4.1.0",
    "browser-sync": "^2.0.1",
    "browserify": "^8.1.3",
    "uglify-js": "^2.4.16",
    "watchify": "^2.3.0"
  }
}
